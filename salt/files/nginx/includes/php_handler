# Common PHP configuration
# ------------------------
# Specify the following in the server configuration file wherever PHP is expected
	## [Enable PHP Handler]
	#include includes/php_common;
	#

# PHP handling
location ~* \.php$ {
	try_files $uri =404;
	fastcgi_pass unix:/var/run/php/php7.0-fpm.sock;
	fastcgi_index index.php;
	include fastcgi.conf;
}
